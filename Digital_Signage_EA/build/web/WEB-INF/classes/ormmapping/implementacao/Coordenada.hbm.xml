<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="implementacao.Coordenada" table="Coordenada" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="_player" column="PlayerID" class="implementacao.Player" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="Latitude" column="Latitude" type="double" not-null="true" lazy="false"/>
		<property name="Longitude" column="Longitude" type="double" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
