<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="implementacao.Cliente" table="Cliente" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="Nome" column="Nome" type="string" length="255" not-null="false" lazy="false"/>
		<property name="NIF" column="NIF" type="integer" length="10" not-null="false" lazy="false"/>
		<property name="Email" column="Email" type="string" length="255" not-null="false" lazy="false"/>
		<property name="Password" column="Password" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM__rede" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="ClienteID" not-null="true"/>
			<one-to-many class="implementacao.Rede"/>
		</set>
		<set name="ORM__canal" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="ClienteID" not-null="true"/>
			<one-to-many class="implementacao.Canal"/>
		</set>
	</class>
</hibernate-mapping>
