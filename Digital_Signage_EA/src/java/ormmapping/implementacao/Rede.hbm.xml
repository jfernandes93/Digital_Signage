<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="implementacao.Rede" table="Rede" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="_cliente" cascade="lock" column="ClienteID" class="implementacao.Cliente" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="Descricao" column="Descricao" type="string" length="255" not-null="false" lazy="false"/>
		<property name="Nome" column="Nome" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM__player" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="RedeID" not-null="true"/>
			<one-to-many class="implementacao.Player"/>
		</set>
	</class>
</hibernate-mapping>
